        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToGo"
        mc:Ignorable="d"
        Title="RoomMenu" Height="1100" Width="1200" >
    <Viewbox Stretch="Fill">
        <Grid Height="1069" Width="1192" >
            <ScrollViewer Height="750" Width="800">
                <StackPanel Orientation="Vertical" x:Name="StackOfRoom" Background="LightGray" Margin="2"/>

            </ScrollViewer>
            <Label x:Name="titlename" FontSize="60" Content="Label" HorizontalAlignment="Left" Margin="99,59,0,0" VerticalAlignment="Top" Width="606"/>

        </Grid>
    </Viewbox>
</Window>
